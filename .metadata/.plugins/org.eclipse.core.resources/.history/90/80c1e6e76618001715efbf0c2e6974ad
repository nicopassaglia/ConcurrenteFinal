package Testing;

import static org.junit.Assert.*;

import org.junit.Test;

import Extra.Matriz;

public class UnitTestMatriz {

	
	
	@Test
	public void SumaMatrices(Matriz A, Matriz B){
		A = new Matriz(1,1);
		B = new Matriz(1,1);
		
		A.setDato(0, 0, 1);
		B.setDato(0, 0, 2);
		
		Matriz C = new Matriz(1,1);
		C.setDato(0, 0, 3);
		
		assertEquals(C,A.plus(B));
	}
}
