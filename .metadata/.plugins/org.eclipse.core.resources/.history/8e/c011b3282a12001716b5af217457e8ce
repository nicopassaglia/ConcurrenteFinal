package Maquinas;

import Instrumentos.Instrumento;

public class Maquina extends Instrumento implements Runnable{
	
	public Maquina(){
		super();
	}

	@Override
	public void Disparar() {
		// TODO Auto-generated method stub
		System.out.println("Me dispare" + this.getClass().getName());
		
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		Disparar();
		
	}



	
	



}
